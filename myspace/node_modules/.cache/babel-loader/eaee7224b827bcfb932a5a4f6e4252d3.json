ul>\r\n          <ul class=\"navbar-nav \" v-else>\r\n          <router-link class=\"nav-link\" :to=\"{name:'chesslist'}\">棋局列表</router-link>\r\n          </ul>\r\n        </li>\r\n       <!--<li class=\"nav-item\">\r\n          <router-link class=\"nav-link\" :to=\"{name:'userprofile',params:{userId:1}}\">用户动态</router-link>\r\n        </li>-->\r\n      </ul>\r\n      <!--取反，如果未登录展示登录与注册-->\r\n       <ul class=\"navbar-nav \" v-if=\"!$store.state.user.is_login\">\r\n        <li class=\"nav-item\">\r\n          <router-link class=\"nav-link \" :to=\"{name:'login'}\">登录</router-link>\r\n        </li>\r\n        <li class=\"nav-item\">\r\n          <router-link class=\"nav-link\" :to=\"{name:'register'}\">注册</router-link>\r\n        </li>\r\n      </ul>\r\n      <!--登录成功显示姓名和退出，并转到登录界面-->\r\n      <ul class=\"navbar-nav \" v-else>\r\n        <li class=\"nav-item\">\r\n          <!--<router-link class=\"nav-link \" :to=\"{name:'userprofile',params:{userId:$store.state.user.id}}\">{{$store.state.user.username}}</router-link>-->\r\n          <router-link class=\"nav-link \" :to=\"{name:'userprofile',params:{userId:$store.state.user.id}}\">{{$store.state.user.username}}</router-link>\r\n        </li>\r\n        <li class=\"nav-item\">\r\n          <router-link class=\"nav-link\" :to=\"{name:'login'}\" style=\"cursor: pointer;\" @click=\"logout\">退出</router-link>\r\n        </li>\r\n      </ul>\r\n\r\n    </div>\r\n  </div>\r\n</nav>\r\n</template>\r\n\r\n<script>\r\nimport { useStore } from 'vuex';\r\nexport default {\r\n    name:\"NavBar\",\r\n    //退出删除掉jwt的事件\r\n    setup(){\r\n      const store=useStore();\r\n      const logout=()=>{\r\n        store.commit('logout');\r\n      };\r\n      return {\r\n        logout,\r\n      }\r\n    }\r\n    \r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.nav-color{\r\n  background-color: rgb(38,38,38);\r\n}\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}